{
  "name": "@mohayonao/midi-keyboard",
  "description": "JavaScript utility for MIDI keyboard",
  "version": "0.2.1",
  "author": "Nao Yonamine <mohayonao@gmail.com>",
  "bugs": {
    "url": "https://github.com/mohayonao/midi-keyboard/issues"
  },
  "dependencies": {
    "@mohayonao/midi-device": "^0.4.1",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "babel-eslint": "^3.1.20",
    "browserify": "^10.2.4",
    "eslint": "^0.24.0",
    "espower-babel": "^3.2.0",
    "isparta": "^3.0.3",
    "mocha": "^2.2.5",
    "power-assert": "^0.11.0",
    "sinon": "^1.15.4"
  },
  "files": [
    "package.json",
    "README.md",
    "index.js",
    "webmidi.js",
    "lib"
  ],
  "homepage": "https://github.com/mohayonao/midi-keyboard/",
  "keywords": [
    "midi"
  ],
  "license": "MIT",
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohayonao/midi-keyboard.git"
  },
  "scripts": {
    "build": "npm run build-to5 && npm run build-browser",
    "build-browser": "browserify ./webmidi.js --standalone MIDIKeyboard --outfile build/midi-keyboard.js",
    "build-to5": "babel src --out-dir lib",
    "cover": "babel-node $(npm bin)/isparta cover --report text --report html --report lcov _mocha",
    "lint": "eslint src test",
    "prepublish": "rm -rf lib && npm run lint && npm run test && npm run build-to5",
    "test": "mocha --compilers js:espower-babel/guess",
    "travis": "npm run lint && npm run test"
  }
}
